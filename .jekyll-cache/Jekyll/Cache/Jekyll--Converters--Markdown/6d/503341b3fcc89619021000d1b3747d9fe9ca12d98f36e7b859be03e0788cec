        &lt;a href=&quot;https://twitter.com/githubstatus&quot;&gt;@githubstatus&lt;/a&gt;
      &lt;/div&gt;

      &lt;a href=&quot;/&quot; class=&quot;logo logo-img-1x&quot;&gt;
